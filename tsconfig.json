{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/(pages)/join/1/page.jsx",
    "src/app/(pages)/join/2/page.jsx",
    "src/app/(pages)/join/3/page.jsx",
    "src/app/(pages)/login/page.jsx",
    "src/components/Nav/Profile.jsx",
    "src/app/(pages)/join/5/page.jsx",
    "src/app/(pages)/admin/user-approval/page.jsx",
    "src/provider/JoinProvider.jsx"
, "src/app/(pages)/join/3-1/page.jsx"  ],
  "exclude": ["node_modules"]
}
